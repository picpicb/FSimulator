
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET           /api/flights            controllers.FlightController.index
GET           /api/flights/:id        controllers.FlightController.read(id: String)
POST          /api/flights            controllers.FlightController.create
PUT           /api/flights/:id        controllers.FlightController.update(id: String)
DELETE        /api/flights/:id        controllers.FlightController.remove(id: String)

      